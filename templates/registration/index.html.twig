{% extends 'base.html.twig' %}

{% block title %}Registration{% endblock %}

{% block body %}
    <section>



        <div class="container py-sm-3 my-3 border col-md-4  text">

            <h3>Konto erstellen </h3>

            {{ form_start(form) }}
            <div class="mb-8">
                {{ form_errors(form) }}


                <div class="row mp-3">

                        {{ form_row(form.email) }}

                </div>
                <div class="row mb-1 ">

                    {{ form_row(form.password) }}

                </div>
                <div class="row mt-1">
                    {{ form_end(form) }}
                </div>

            </div>

        </div>
    </section>
{% endblock %}
